[user]
  name = Ramin Keene
  email = raminkeene@mac.com

[github]
  user = ramin

[core]
  excludesfile = ~/.gitignore_global
  editor = emacs

[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[color "diff-highlight"]
  oldNormal = "red bold"
  oldHighlight = "red bold 52"
  newNormal = "green bold"
  newHighlight = "green bold 22"

[credential]
  helper = osxkeychain

[grep]
  lineNumber = true

[mergetool]
  prompt = false

[push]
  default = simple
